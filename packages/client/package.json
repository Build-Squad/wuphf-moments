{
  "name": "WUPHF-For-Moments-client",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@onflow/fcl": "^1.3.2",
    "buffer": "^6.0.3",
    "fomantic-ui-css": "^2.9.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-use-websocket": "^4.3.1",
    "semantic-ui-react": "^2.1.4",
    "web-vitals": "^2.1.0",
    "websocket": "^1.0.34"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^13.0.0",
    "@testing-library/user-event": "^13.2.1",
    "@types/jest": "^27.0.1",
    "@types/node": "^16.7.13",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "react-scripts": "5.0.1",
    "typescript": "^4.4.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "es2021": true
    },
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "settings": {
      "react": {
        "version": "detect"
      }
    },
    "rules": {
      "max-len": [
        "warn",
        {
          "code": 120,
          "ignoreComments": true,
          "ignoreUrls": true
        }
      ],
      "@typescript-eslint/quotes": [
        "error",
        "single",
        {
          "allowTemplateLiterals": true
        }
      ],
      "react/jsx-curly-spacing": [
        "error",
        {
          "when": "always",
          "allowMultiline": true,
          "children": true,
          "spacing": {
            "objectLiterals": "never"
          }
        }
      ],
      "react-hooks/exhaustive-deps": "warn"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
